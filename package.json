{
  "name": "zeed",
  "version": "0.3.10",
  "description": "Simple foundation library",
  "keywords": [
    "foundation",
    "typescript",
    "event",
    "logging"
  ],
  "license": "MIT",
  "author": {
    "name": "Dirk Holtwick",
    "url": "https://holtwick.de"
  },
  "type": "module",
  "typings": "dist/esm/index.all.d.ts",
  "exports": {
    "./ts": "./src/index.browser.ts",
    "./browser": "./dist/esm/index.browser.js",
    "./log": "./dist/esm/index.log.js",
    "./dist/": "./dist/",
    ".": {
      "browser": "./dist/esm/index.browser.js",
      "node": "./dist/esm/index.node.js",
      "default": "./dist/esm/index.browser.js"
    }
  },
  "module": "dist/esm/index.browser.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "start": "npm run watch",
    "build": "npm run clean && npm run build:esm",
    "build:esm": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "watch": "tsc -w -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "jest src"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "jest": "^27.0.6",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
