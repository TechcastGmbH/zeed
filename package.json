{
  "name": "zeed",
  "version": "0.8.15",
  "description": "🌱 Simple foundation library",
  "license": "MIT",
  "keywords": [
    "foundation",
    "typescript",
    "eventemitter",
    "logging",
    "kit",
    "lib",
    "utils",
    "zeed"
  ],
  "author": {
    "name": "Dirk Holtwick",
    "email": "dirk.holtwick@gmail.com",
    "url": "https://holtwick.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holtwick/zeed.git"
  },
  "bugs": {
    "url": "https://github.com/holtwick/zeed/issues"
  },
  "funding": {
    "type": "GitHub Sponsors ❤",
    "url": "https://github.com/sponsors/holtwick"
  },
  "homepage": "https://github.com/holtwick/zeed",
  "type": "module",
  "sideEffects": true,
  "exports": {
    ".": {
      "types": "./dist/index.all.d.ts",
      "browser": "./dist/index.browser.js",
      "require": "./dist/index.node.cjs",
      "node": "./dist/index.node.js",
      "default": "./dist/index.browser.js"
    }
  },
  "main": "dist/index.node.cjs",
  "module": "dist/index.browser.js",
  "types": "dist/index.all.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.13.1"
  },
  "scripts": {
    "start": "npm run watch",
    "clean": "rm -rf dist",
    "check": "tsc --noEmit -p tsconfig.json",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup src/index.all.ts --dts-only && tsup src/index.browser.ts src/index.node.ts --format esm,cjs --dts --sourcemap --minify --keep-names",
    "watch": "npm run build:tsup -- --watch",
    "prepublishOnly": "npm run build && npm run test:publish",
    "coverage": "vitest src --global --run --coverage",
    "test": "vitest src --globals --run",
    "test:publish": "vitest src --globals --run"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "c8": "^7.12.0",
    "cross-fetch": "^3.1.5",
    "esbuild": "^0.15.5",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  }
}
