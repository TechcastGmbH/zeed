{
  "name": "zeed",
  "version": "0.7.119",
  "description": "🌱 Simple foundation library",
  "license": "MIT",
  "keywords": [
    "foundation",
    "typescript",
    "event",
    "logging"
  ],
  "author": {
    "name": "Dirk Holtwick",
    "email": "dirk.holtwick@gmail.com",
    "url": "https://holtwick.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holtwick/zeed.git"
  },
  "bugs": {
    "url": "https://github.com/holtwick/zeed/issues"
  },
  "funding": {
    "type": "GitHub Sponsors ❤",
    "url": "https://github.com/sponsors/holtwick"
  },
  "homepage": "https://github.com/holtwick/zeed",
  "type": "module",
  "typings": "dist/index.all.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/index.browser.js",
      "require": "./dist/index.node.cjs",
      "node": "./dist/index.node.js",
      "default": "./dist/index.browser.js"
    }
  },
  "module": "dist/index.browser.js",
  "main": "dist/index.node.cjs",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.13.1"
  },
  "scripts": {
    "start": "npm run watch",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup src/index.*.ts --format esm,cjs --dts --sourcemap --keep-names",
    "watch": "npm run build:tsup -- --watch",
    "prepublishOnly": "npm run build && npm run test:publish",
    "test": "jest src --coverage",
    "test:publish": "jest src --detectOpenHandles"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "cross-fetch": "^3.1.5",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  }
}
