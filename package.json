{
  "name": "zeed",
  "version": "0.4.0",
  "description": "🌱 Simple foundation library",
  "keywords": [
    "foundation",
    "typescript",
    "event",
    "logging"
  ],
  "funding": {
    "type": "GitHub Sponsors ❤",
    "url": "https://github.com/sponsors/holtwick"
  },
  "license": "MIT",
  "author": {
    "name": "Dirk Holtwick",
    "url": "https://holtwick.de"
  },
  "type": "module",
  "typings": "dist/esm/index.all.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/esm/index.browser.js",
      "require": "./dist/cjs/index.node.js",
      "node": "./dist/esm/index.node.js",
      "default": "./dist/esm/index.browser.js"
    }
  },
  "module": "dist/esm/index.browser.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.13.1"
  },
  "scripts": {
    "start": "npm run watch",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json && echo '{\"type\":\"commonjs\"}' >> dist/cjs/package.json",
    "build:tsup": "tsup src/index.*.ts --dts --sourcemap --format esm,cjs --keep-names --minify-whitespace",
    "clean": "rm -rf dist",
    "watch": "tsc -w -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "jest src"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "jest": "^27.0.6",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "ts-jest": "^27.0.3",
    "tsup": "^4.12.5",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}