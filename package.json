{
  "name": "zeed",
  "version": "0.7.81",
  "description": "🌱 Simple foundation library",
  "keywords": [
    "foundation",
    "typescript",
    "event",
    "logging"
  ],
  "funding": {
    "type": "GitHub Sponsors ❤",
    "url": "https://github.com/sponsors/holtwick"
  },
  "license": "MIT",
  "author": {
    "name": "Dirk Holtwick",
    "url": "https://holtwick.de"
  },
  "type": "module",
  "typings": "dist/index.all.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/index.browser.js",
      "require": "./dist/index.node.cjs",
      "node": "./dist/index.node.js",
      "default": "./dist/index.browser.js"
    }
  },
  "module": "dist/index.browser.js",
  "main": "dist/index.node.cjs",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.13.1"
  },
  "scripts": {
    "start": "npm run watch",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup src/index.*.ts --dts --sourcemap --format esm,cjs --keep-names --minify-whitespace",
    "watch": "npm run build:tsup -- --watch",
    "prepublishOnly": "npm run build && npm test",
    "test": "jest src"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "cross-fetch": "^3.1.4",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "tsup": "^5.10.0",
    "typescript": "^4.5.2"
  }
}
